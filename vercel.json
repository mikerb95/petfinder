{
  "routes": [
    { "src": "^/$", "dest": "/public/index.html" },
  { "src": "^/shop/?$", "dest": "/api/index.js" },
  { "src": "^/shop/([A-Za-z0-9-_]+)/?$", "dest": "/api/index.js" },
    { "src": "^/dashboard$", "dest": "/api/index.js" },
  { "src": "^/admin/products/?$", "dest": "/api/index.js" },
    { "src": "^/assets/(.*)$", "dest": "/public/assets/$1" },
  { "src": "^/tech/?$", "dest": "/api/index.js" },
  { "src": "^/license/?$", "dest": "/api/index.js" },
  { "src": "^/m/?$", "dest": "/api/index.js" },
    { "src": "^/api/(.*)$", "dest": "/api/index.js" },
    { "src": "^/p/(.*)$", "dest": "/api/index.js" },
  { "src": "^/(login|register|scan|terms|kickoff|privacy|forgot|reset)$", "dest": "/api/index.js" },
    { "src": ".*", "status": 404, "dest": "/public/404.html" }
  ]
}
