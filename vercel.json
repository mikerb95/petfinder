{
  "routes": [
    { "src": "^/$", "dest": "/public/index.html" },
  { "src": "^/blog/?$", "dest": "/api/index.js" },
  { "src": "^/blog/editor/?$", "dest": "/api/index.js" },
  { "src": "^/blog/([A-Za-z0-9-_]+)/?$", "dest": "/api/index.js" },
  { "src": "^/shop/?$", "dest": "/api/index.js" },
  { "src": "^/shop/([A-Za-z0-9-_]+)/?$", "dest": "/api/index.js" },
  { "src": "^/bnb/?$", "dest": "/api/index.js" },
  { "src": "^/cart/?$", "dest": "/api/index.js" },
  { "src": "^/checkout/?$", "dest": "/api/index.js" },
  { "src": "^/payment/?$", "dest": "/api/index.js" },
  { "src": "^/order_confirmed/?$", "dest": "/api/index.js" },
  { "src": "^/order_lookup/?$", "dest": "/api/index.js" },
  { "src": "^/contact/?$", "dest": "/api/index.js" },
    { "src": "^/dashboard/?$", "dest": "/api/index.js" },
  { "src": "^/admin/products/?$", "dest": "/api/index.js" },
  { "src": "^/admin/blog/?$", "dest": "/api/index.js" },
    { "src": "^/assets/(.*)$", "dest": "/public/assets/$1" },
  { "src": "^/tech/?$", "dest": "/api/index.js" },
  { "src": "^/license/?$", "dest": "/api/index.js" },
  { "src": "^/m/?$", "dest": "/api/index.js" },
  { "src": "^/manifest\\.webmanifest$", "dest": "/public/manifest.webmanifest" },
  { "src": "^/sw\\.js$", "dest": "/public/sw.js" },
    { "src": "^/api/(.*)$", "dest": "/api/index.js" },
    { "src": "^/p/(.*)$", "dest": "/api/index.js" },
  { "src": "^/(login|register|scan|terms|kickoff|privacy|forgot|reset)/?$", "dest": "/api/index.js" },
    { "src": ".*", "status": 404, "dest": "/public/404.html" }
  ]
}
